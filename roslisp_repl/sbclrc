
;;; asdf source registry entries
(require 'asdf)
(pushnew (format nil "/opt/ros/~a/share/common-lisp/source/"
                 (string-downcase (sb-unix::posix-getenv "ROS_DISTRO")))
         asdf:*central-registry* :test #'equal)
(pushnew (format nil "/opt/ros/~a/share/common-lisp/source/slime/"
                 (string-downcase (sb-unix::posix-getenv "ROS_DISTRO")))
         asdf:*central-registry* :test #'equal)
